<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crawlerdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.crawlerdemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.crawlerdemo.DisplayCrawlActivity"
            android:label="@string/title_activity_display_crawl" >
        </activity>
        <activity
            android:name="com.example.crawlerdemo.CrawlParameters"
            android:label="@string/title_activity_crawl_parameters" >
        </activity>
        <activity
            android:name="com.example.crawlerdemo.CrawlNoGo"
            android:label="@string/title_activity_crawl_no_go" >
        </activity>
        <activity
            android:name="com.example.crawlerdemo.CrawlNoGo2"
            android:label="@string/title_activity_crawl_no_go2" >
        </activity>
        <activity
            android:name="com.example.crawlerdemo.CrawlNoGo3"
            android:label="@string/title_activity_crawl_no_go3" >
        </activity>
        <activity
            android:name="com.example.crawlerdemo.CrawlFinalize"
            android:label="@string/title_activity_crawl_finalize" >
        </activity>
        <activity
            android:name="com.example.crawlerdemo.StartCrawl"
            android:label="@string/title_activity_start_crawl" >
        </activity>
        <activity
            android:name="com.example.crawlerdemo.CheckIn"
            android:label="@string/title_activity_check_in" >
        </activity>
        <activity
            android:name="com.example.crawlerdemo.ChallengePost"
            android:label="@string/title_activity_challenge_post" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

</manifest>
